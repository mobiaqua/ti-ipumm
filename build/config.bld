/**********  PSI_KPI instrumentation ***********/
if (prof_type == "ENABLE")
{
    var psi_kpi_flag = true
    print("Profiler enabled");
    target.ccOpts.suffix += " -DBUILD_PSI_KPI";
}
else
{
    var psi_kpi_flag = false
}
