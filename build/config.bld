/*****************  Tracing ********************/

if(trace_level == 0) {
    target.ccOpts.suffix += " -DDCE_DEBUG_LEVEL=0";
} else if(trace_level == 1) {
    target.ccOpts.suffix += " -DDCE_DEBUG_ENABLE";
    target.ccOpts.suffix += " -DDCE_DEBUG_LEVEL=1";
} else if(trace_level == 2) {
    target.ccOpts.suffix += " -DDCE_DEBUG_ENABLE";
    target.ccOpts.suffix += " -DDCE_DEBUG_LEVEL=2";
} else if(trace_level == 3) {
    target.ccOpts.suffix += " -DDCE_DEBUG_ENABLE";
    target.ccOpts.suffix += " -DDCE_DEBUG_LEVEL=3";
} else if(trace_level >= 4) {
    target.ccOpts.suffix += " -DDCE_DEBUG_ENABLE";
    target.ccOpts.suffix += " -DDCE_DEBUG_LEVEL=4";
}

/*****************  Tracing ********************/


var omap5_zebu = false;
var omap5_virtio = false;
var build_omap5 = false;
var omap5_es20 = false;
var build_vayu = false;
var hw_OMAP4 = 0;
var hw_OMAP5 = 1;
var hw_VAYU = 2;

var VIRTIO = 0;
var ZEBU = 0;
var ES10 = 1;
var ES20 = 2;
var HwType = hw_OMAP4; //default omap4

var HwVer = ES20; //Default ES2.0
SMPmode = true;
if (hdw_type == "OMAP5")
{
    build_omap5 = true;
    HwType = hw_OMAP5;
    target.ccOpts.suffix += " -DBUILD_FOR_OMAP5";
    HwVer = ES20;
    omap5_es20 = true;
    target.ccOpts.suffix += " -DOMAP5432_ES20";
    target.platform = target.platforms[7];
}
else if (hdw_type == "VAYU")
{
    HwType = hw_VAYU;
    HwVer = ES10;
    var build_vayu = true;
    target.ccOpts.suffix += " -DBUILD_FOR_VAYU";
    target.ccOpts.suffix += " -DVAYU_ES10";
    target.platform = target.platforms[8];
}
else
{
    HWType = hw_OMAP4;
    var build_omap4 = true;
    HwVer = ES20;
    target.ccOpts.suffix += " -DBUILD_FOR_OMAP4";
    target.platform = target.platforms[6];
}

Build.targets.$add(target);

/* Eliminate profiles that aren't 'debug' or 'release' */
for (var t = 0; t < Build.targets.length; t++) {
    for (prof in Build.targets[t].profiles) {
        if ((prof != "debug") && (prof != "release")) {
            delete Build.targets[t].profiles[prof];
        }
    }
}

